<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name = "Suite">
    <test name = "Cross Browser Test FF">
        <parameter name = "browser" value = "firefox"/>
        <classes>
            <class name = "com.selenium_min_moodle.CrossBrowserTest"/>
        </classes>
    </test>
    <test name = "Cross Browser Test Chrome">
        <parameter name = "browser" value = "chrome"/>
        <classes>
            <class name = "com.selenium_min_moodle.CrossBrowserTest"/>
        </classes>
    </test>
    <test name = "Edit Announcement Test">
        <classes>
            <class name = "com.selenium_min_moodle.EditAnnouncementTest"/>
        </classes>
    </test>
    <test name = "Delete Announcement Test">
        <classes>
            <class name = "com.selenium_min_moodle.DeleteAnnouncementTest"/>
        </classes>
    </test>
    <test name = "Create Task Test">
        <classes>
            <class name = "com.selenium_min_moodle.CreateTaskTest"/>
        </classes>
    </test>
    <test name = "File Upload Test">
        <classes>
            <class name = "com.selenium_min_moodle.UploadFileTest"/>
        </classes>
    </test>
    <test name = "Language Test">
        <classes>
            <class name = "com.selenium_min_moodle.LanguageTest"/>
        </classes>
    </test>
    <test name = "Role Test">
        <classes>
            <class name = "com.selenium_min_moodle.SwitchRoleTest"/>
        </classes>
    </test>
</suite>